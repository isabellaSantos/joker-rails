<div class="container-fluid">
  <div class="row-fluid">
    <div class="span12">
      <h3><%= '<%=' %> t('activerecord.models.<%= singular_table_name %>').capitalize %> #<%= '<%=' %> @<%= singular_table_name %>.id %></h3>
    </div>
  </div>
</div>

<dl>
  <% attributes_names.each do |attribute| %>
  <dt><%= attribute.humanize %>:</dt>
  <dd><%= '<%=' %> @<%= singular_table_name %>.<%= attribute %> %></dd>
  <% end %>
</dl>
